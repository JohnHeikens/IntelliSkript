	patterns:
		[the] glowing of %entities%
		%entities%'[s] glowing
	set:
#		(internal code)
	reset:
#		(internal code)
	return type: boolean
#Gravity
#If entity is affected by gravity or not, i.e. if it has Minecraft 1.10+ NoGravity flag.
