#on death:
#	set the death message to "%player% died!"
expression:
	patterns:
		[the] [chat( |-)]message
		[the] (join|log[ ]in)( |-)message
		[the] (quit|leave|log[ ]out|kick)( |-)message
		[the] death( |-)message
