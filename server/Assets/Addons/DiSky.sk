#RetrieveInvite
effect:
	patterns:
		retrieve invite (with|from) id %string% (from|with|of|in) %guild% [(with|using) [the] [bot] %bot%] and store (it|the invite) in %object%

#RetrieveInvites
effect:
	patterns:
