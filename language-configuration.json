{
	"comments": {
	  "lineComment": "#",
	},
	"brackets": [
	  ["{", "}"],
	  ["[", "]"],
	  ["(", ")"]
	],
	"autoClosingPairs": [
	  { "open": "{", "close": "}", "notIn": ["string", "comment"] },
	  { "open": "[", "close": "]", "notIn": ["string", "comment"] },
	  { "open": "(", "close": ")", "notIn": ["string", "comment"] },
	  { "open": "%", "close": "%", "notIn": ["comment"] },
	  { "open": "\"", "close": "\"", "notIn": ["comment"] },
	],
	"autoCloseBefore": ";:.,=}])>` \n\t",
	"surroundingPairs": [
	  ["{", "}"],
	  ["[", "]"],
	  ["(", ")"],
	  ["\"", "\""],
	],
	"indentationRules": {
	  "increaseIndentPattern": "^.*:\\s*($|(?<!#)#(?!#))"
	}
  }