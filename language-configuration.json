{
	"indentationRules": {
		"increaseIndentPattern": "^.*:\\s*($|(?<!#)#(?!#))",
		"decreaseIndentPattern": "decrease indent"
	},
	"comments": {
	  "lineComment": "#",
	},
	"autoClosingPairs": [
		{ "open": "{", "close": "}", "notIn": ["string", "comment"] },
		{ "open": "[", "close": "]", "notIn": ["string", "comment"] },
		{ "open": "(", "close": ")", "notIn": ["string", "comment"] },
		{ "open": "%", "close": "%", "notIn": ["comment"] },
		{ "open": "\"", "close": "\"", "notIn": ["comment"] },
	  ],
  }